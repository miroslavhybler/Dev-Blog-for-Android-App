    <string name="settings_changelog_title_1_0_0">First release 🚀</string>
    <string name="settings_changelog_1_0_0__0">Loading posts and post detail 📖</string>
    <string name="settings_changelog_1_0_0__1">Fully adaptive 📱</string>
    <string name="settings_changelog_1_0_0__2">Checking for new posts every two days on background using WorkManager</string>

    <string name="settings_changelog_title_1_0_1">Cache support &amp; Bugfixes 🐞</string>
    <string name="settings_changelog_1_0_1__0">Added cache support for posts</string>
    <string name="settings_changelog_1_0_1__1">Updated Content synchronization, enabled to load data from local when offline</string>
    <string name="settings_changelog_1_0_1__2">Updated side cases and error scenarios</string>
    <string name="settings_changelog_1_0_1__3">Other minor fixes</string>

    <string name="settings_changelog_title_1_0_2">Bugfix of mismatched links🐞</string>
    <string name="settings_changelog_1_0_2__0">Fixed loading list of posts, handling scenario there is featured post</string>
    <string name="settings_changelog_1_0_2__1">Data are loaded from remote when app is opened independently on ContentSyncWorker</string>

    <string name="settings_changelog_title_1_0_3">Performance improvements ⚡️</string>
    <string name="settings_changelog_1_0_3__0">Added baseline profiles for opening article user flow</string>
    <string name="settings_changelog_1_0_3__1">Added benchmarks for performance analysis</string>
    <string name="settings_changelog_1_0_3__2">Added "new" mark for unread posts</string>

    <string name="settings_changelog_title_1_0_4">Edge cases ⚠️</string>
    <string name="settings_changelog_1_0_4__0">Better Api edge cases error handling, separate UI for unexpected api errors and expected errors from html parsing</string>
    <string name="settings_changelog_1_0_4__1">Loaded articles are now saved in files dir instead of cache dir</string>
    <string name="settings_changelog_1_0_4__2">Added ability to refresh data by pull refresh</string>

    <string name="settings_changelog_title_1_1_0">UX improvements 📱</string>
    <string name="settings_changelog_1_1_0__0">Added VerticalDragHandle to HomeScreen to enable resize HomeListPane and PostPane</string>
    <string name="settings_changelog_1_1_0__1">Better format of codes in posts on PostPane</string>

    <string name="settings_changelog_title_1_1_1">More UX improvements 📈</string>
    <string name="settings_changelog_1_1_1__0">Added ability to open and zoom images in post</string>
    <string name="settings_changelog_1_1_1__1">Added ability to enable/disable cellular data usage</string>
    <string name="settings_changelog_1_1_1__2">Fixed selection of post section from Contest screen</string>
    <string name="settings_changelog_1_1_1__3">Other minor UI changes</string>

